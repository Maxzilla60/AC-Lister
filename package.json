	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.15.4",
		"ajv": "^8.8.2",
		"bowser": "^2.11.0",
		"canvas-confetti": "^1.4.0",
		"canvas-to-blob": "^1.0.0",
		"file-saver": "^2.0.5",
		"hammerjs": "^2.0.8",
		"html2canvas": "^1.4.0",
		"nanoid": "^3.1.32",
		"rxjs": "^7.5.2"
	},
	"devDependencies": {
		"@types/canvas-confetti": "^1.4.2",
		"@types/file-saver": "^2.0.5",
