{
	"name": "ac-lister",
	"version": "2.0.0",
	"description": "Animal Crossing Villager Lister",
	"private": true,
	"scripts": {
		"start": "parcel index.html",
		"build": "parcel build index.html",
		"codenvy": "parcel index.html --host 0.0.0.0 --port 1337",
		"cypress": "npx cypress open",
